<!DOCTYPE html>

<html lang="en">
	
	<head>
    		<meta charset="utf-8" />
    		<meta name="viewport" content="width=device-width, initial-scale=1.0" />

	<link rel="stylesheet" href="https://cdn.plyr.io/3.5.6/plyr.css" />

	</head>
	
<body>

	<video controls crossorigin playsinline poster="https://static.wixstatic.com/media/d03561_544971eb3f8a471ca902229fc57954e0~mv2.jpg" class="js-player">
		
	<source
    
    		src="https://video.wixstatic.com/video/d5df8a_bff44ad33efb4858a759513812934664/480p/mp4/file.mp4"
   		type="video/mp4"
    		size="480"
    
    	/>   
		
	<source
    
    		src="https://video.wixstatic.com/video/d5df8a_bff44ad33efb4858a759513812934664/720p/mp4/file.mp4"
   		type="video/mp4"
    		size="720"
    
    	/>   

    	<source
    
    		src="https://video.wixstatic.com/video/d5df8a_bff44ad33efb4858a759513812934664/1080p/mp4/file.mp4"
   		type="video/mp4"
    		size="1080"
    
    	/>   
    	 
    	   
    
	</video>


	</script>

		<script src="https://cdn.plyr.io/3.5.6/plyr.js" crossorigin="anonymous">

	</script>

	<script>

    	document.addEventListener('DOMContentLoaded', () => {
        
        const controls = [
            'play-large', 
            'rewind', 
            'play', 
            'fast-forward',
            'progress', 
            'current-time', 
            'duration', 
            'mute', 
            'volume', 
            'captions', 
            'settings', 
            'pip', 
            'fullscreen' 
        ];

        const player = Plyr.setup('.js-player', { controls });

    });
    
	</script>
    
</body>
</html>
